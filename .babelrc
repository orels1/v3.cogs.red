{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"],
        "node": "current"
      }
    }],
    "stage-2"
  ],
  "plugins": [
    "transform-runtime",
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-vue-jsx"
  ],
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": [
        "transform-vue-jsx", 
        "transform-es2015-modules-commonjs",
        "dynamic-import-node",
        "transform-decorators-legacy",
        "transform-class-properties"
      ]
    }
  }
}

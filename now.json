{
  "version": 2,
  "name": "v3.cogs.red",
  "builds": [
    {
      "src": "now-build.sh",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    {
      "src": "^/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "^/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "^/img/(.*)",
      "dest": "/img/$1"
    },
    {
      "src": "^/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "^/robots.txt",
      "dest": "/robots.txt"
    },
    {
      "src": "^/logo.png",
      "dest": "/logo.png"
    },
    {
      "src": "^/(.*).js",
      "dest": "/$1.js"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ]
}